

<% if (result && result.length) { %>
    <ul>
        <% result.forEach((item) => { %> <!-- Change 'res' to 'item' to avoid confusion -->
            <li>
                <h2><%= item.name %></h2> <!-- Use 'item' instead of 'res' -->
                <img src="<%= item.image %>" alt="<%= item.name %>" />
                <p>ID: <%= item.id %></p>
            </li>
        <% }); %>
    </ul>
<% } else { %>
    <p>No results found.</p>
<% } %>
